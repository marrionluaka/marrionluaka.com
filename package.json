{
  "name": "nuxt_blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel develop watch:styl",
    "develop": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "npm-run-all --parallel build:css deploy",
    "deploy": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "watch:styl": "stylus -w assets/styl/styles.styl -o assets/css/styles.css",
    "build:css": "npm-run-all compile:styl prefix:css compress:css",
    "compile:styl": "stylus assets/styl/styles.styl -o assets/css/styles.prod.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' assets/css/styles.prod.css -o assets/css/styles.prod.css",
    "compress:css": "stylus -c assets/css/styles.prod.css"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/style-resources": "1.0.0",
    "@types/ramda": "0.27.38",
    "core-js": "^3.8.3",
    "markdown-it-prism": "2.1.6",
    "nuxt": "^2.14.12",
    "ramda": "0.27.1",
    "share-buttons": "1.7.0",
    "storyblok-markdown-richtext": "1.0.1",
    "storyblok-nuxt": "1.3.0",
    "vue-router": "3.5.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@types/markdown-it": "12.0.1",
    "@vue/composition-api": "1.0.0-rc.5",
    "autoprefixer": "9.8.6",
    "date-fns": "2.19.0",
    "markdown-it": "12.0.4",
    "markdown-it-attrs": "4.0.0",
    "markdown-it-headinganchor": "1.3.0",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.35",
    "postcss-cli": "7.1.2",
    "prismjs": "1.23.0",
    "pug": "3.0.2",
    "pug-loader": "2.4.0",
    "pug-plain-loader": "1.1.0",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.3",
    "vue-pagination-2": "3.0.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "nuxt-fontawesome": "^0.4.0"
  }
}
